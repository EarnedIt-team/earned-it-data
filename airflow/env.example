# Airflow 사용자 ID (macOS의 경우 특정 값 필요)
AIRFLOW_UID=50000

# Airflow 웹 UI 사용자 설정
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# PostgreSQL 설정
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=airflow

# Git 동기화 설정
GIT_SYNC_REPO=https://github.com/username/repo.git
GIT_SYNC_BRANCH=main
GIT_SYNC_WAIT=60
GIT_SYNC_DEST=/git
GIT_SYNC_ROOT=/git
GIT_SYNC_USERNAME=
GIT_SYNC_PASSWORD= 